#!/usr/bin/env m4 
# /etc/fstab   ## filesystems table ### 
#
define(`ROOT',`/l/sd/a/3/b/00/Linux/etc/fstab')dnl
include(ROOT/t)

## system fstab #
include(`/etc/fstab')

## /l/ disks #
DEV(a,0,00) OPTIONS
