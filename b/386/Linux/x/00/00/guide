#!/! /x/00/00/guide

{ for (x in (/x/y/y /x/y/t /x/y/x /x/y/p /x/y/q /x/y/r /x/y/w)) {
	mkdir -p $x;
	for (xx in (00 01 02 x)) {
		test -f $x/$xx || printf '#!/x/fish/x/!\n' > $x/$xx; 
		chmod 777 $x/$xx;
	}
	test -f $x/t || printf '#!/! %s/t\n' $x > $x/t; chmod 777 $x/t;
}}

{
